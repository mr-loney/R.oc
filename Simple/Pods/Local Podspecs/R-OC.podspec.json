{
  "name": "R-OC",
  "version": "0.0.1",
  "summary": "资源索引框架",
  "description": "可方便的使用代码方式获取到工程中所有资源文件，包含（图片，xib，file等）",
  "homepage": "https://github.com/mr-loney/R.oc.git",
  "license": {
    "type": "BSD"
  },
  "source": {
    "git": "R-OC",
    "tag": "0.0.1"
  },
  "authors": {
    "pengjun": "mr_lonely@foxmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "prepare_command": "ruby R-OC/BundleR.rb",
  "default_subspecs": "Source",
  "subspecs": [
    {
      "name": "Source",
      "prefix_header_file": "TinyVideo/TinyVideo-Prefix.pch",
      "requires_arc": true,
      "source_files": [
        "R-OC/R.{h,m}",
        "R-OC/RImage.{h,m}",
        "R-OC/RStoryboard.{h,m}",
        "R-OC/RXib.{h,m}",
        "R-OC/**/*.h"
      ]
    }
  ]
}
